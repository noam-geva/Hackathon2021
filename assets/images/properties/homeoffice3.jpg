(function () {	
	let variUrl = window.location.href;
	let variRegion = variUrl.split('/')[3];
	//Get the div that wraps the language code field
	let languageCodeDivWrapper = document.querySelector('[fs-field-validation-name="languageCode"]');	
	//Store the languageCode field in a variable
	let languageCodeField = languageCodeDivWrapper.querySelector("input");	
		switch (variRegion) {
		  case 'us':
			insertRegionCode('en-us');
			break;
		  case 'eu':
			insertRegionCode('en-gb');
			break;
		  case 'uk':
			insertRegionCode('en-gb');
			break;	
		  case 'au':
			insertRegionCode('en-au');
			break;
		  case 'ca':
			insertRegionCode('en-ca');
			break;	

		  default:
			insertRegionCode('en-us');
		}
	
	function insertRegionCode(regionValue) {
		languageCodeField.value = regionValue;
	}
})();
